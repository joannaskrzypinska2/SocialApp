{
    "semi": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "cursorOffset": -1,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": true,
    "printWidth": 100
  }
  